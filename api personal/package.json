    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
