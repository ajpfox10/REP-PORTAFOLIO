{
  "name": "personalv5-enterprise-api",
  "version": "1.0.0",
  "description": "Enterprise-grade Express + Sequelize API for personalv5 (MySQL), with OpenAPI validation, security hardening, and dynamic CRUD.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run gen:routes && cross-env NODE_ENV=development ts-node-dev --transpile-only --respawn src/server.ts",
    "build": "rimraf dist && npm run gen:routes && tsc -p tsconfig.build.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:integration": "npx cross-env TEST_INTEGRATION=1 NODE_ENV=test jest --runInBand",
    "lint:types": "tsc -p tsconfig.json --noEmit",
    "lint:types:src": "tsc -p tsconfig.build.json --noEmit",
    "lint:types:all": "tsc -p tsconfig.json --noEmit",
    "arranque": "node scripts/arranque.mjs",
    "db:migrate": "ts-node src/scripts/migrate.ts",
    "db:migrate:test": "cross-env NODE_ENV=test ts-node src/scripts/migrate.ts",
    "openapi:export": "ts-node src/types/openapi/export.ts",
    "seed:admin": "node scripts/seedAdmin.mjs",
    "gen:routes": "node scripts/routes/genAutoRoutesManifest.mjs",
    "dev:watch:routes": "chokidar \"src/routes/auto/**/*.routes.ts\" \"src/routes/auto/**/*.routes.js\" -c \"npm run gen:routes\"",
    "lint": "biome check .",
    "format": "biome format .",
    "lint:fix": "biome check . --write",
    "dev:pro": "concurrently -k -n ROUTES,API -c auto \"npm run dev:watch:routes\" \"npm run dev\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,mjs,json}": [
      "biome check --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/compression": "^1.8.1",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.6.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.3",
    "jszip": "^3.10.1",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.0",
    "openapi-request-validator": "^12.1.3",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.3",
    "uuid": "^13.0.0",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.10",
    "@types/supertest": "^2.0.16",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
