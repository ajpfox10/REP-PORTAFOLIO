{
  "name": "personalv5-enterprise-api",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --respawn src/server.ts",
