{
  "name": "nodejs-web-app1",
  "version": "0.0.0",
  "description": "NodejsWebApp1",
  "main": "dist/app.js",
  "author": {
    "name": ""
  },
  "scripts": {
    "build": "npx tsc && npm run copy-static",
    "copy-static": "npx copyfiles -u 1 src/public/**/* dist/public",
    "start": "node dist/app.js",
    "clean": "tsc --build --clean"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.57.0",
    "typescript": "^4.5.2",
    "copyfiles": "^2.4.1"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "cookie-parser": "^1.4.5",
    "body-parser": "^1.19.0"
  }
}
